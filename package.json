{
  "name": "oradorapp",
  "version": "1.0.0",
  "description": "App para ver oradores y eventos locales",
  "main": "index.js",
  "scripts": {
    "start": "node ./server",
    "db:migrate": "knex migrate:latest --knexfile=server/knexfile.js",
    "db:migrate-make": "knex migrate:make --knexfile=server/knexfile.js -- ",
    "db:migrate-rollback": "knex migrate:rollback --knexfile=server/knexfile.js",
    "db:seed": "knex seed:run --knexfile=server/knexfile.js",
    "lint": "eslint \"server/**/*.js\" \"client/**/*.jsx\" \"client/**/*.js\" \"scripts/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-schema": "node ./scripts/update-schema.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GraphqlBA/oradorapp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GraphqlBA/oradorapp/issues"
  },
  "homepage": "https://github.com/GraphqlBA/oradorapp#readme",
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-relay-plugin": "^0.9.3",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "express": "^4.14.0",
    "graphql": "^0.8.2",
    "graphql-relay": "^0.4.4",
    "graphql-server-express": "^0.4.3",
    "graphql-tools": "^0.8.3",
    "history": "^4.4.1",
    "knex": "^0.12.6",
    "normalize.css": "^5.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "v3.0.0-beta.6",
    "react-relay": "^0.9.3",
    "react-router": "v4.0.0-alpha.6",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "casual": "^1.5.8",
    "css-loader": "^0.26.0",
    "eslint": "^3.11.1",
